<% gob6= 0.0 %>

<%igob3=0.0%>

  <% @questions.each do |question| %>
   <% @resultados.each do |resultado| %>
   <% if @formulario.id == resultado.formulario_id%>
      <% if question.id == resultado.question_id  %>
        <% if resultado.question.subcat.nombre == 'GOB1. Participación de grupos de interés' %>
          <%valor = resultado.respuesta.to_f%>
          <% gob1 = gob1 + valor %>
        <% elsif resultado.question.subcat.nombre == 'GOB2. Compromiso Institucional'%>
          <% valor = resultado.respuesta.to_f %>
          <%gob2 = gob2 + valor %>
        <% elsif resultado.question.subcat.nombre == 'GOB3. Plan/Estrategia de Sustentabilidad'%>
          <% if resultado.question.indicador == 'GOB3.1'%>
            <% igob3 = resultado.respuesta %>
          <%elsif resultado.question.indicador != 'GOB3.1'%>
            <% if igob3 == '1.0' %>
              <% valor = resultado.respuesta.to_f %>
              <% gob3 = gob3 + valor  %>
              <%if gob3 > 100 %>
                <% gob3 = 100 %>
              <% elsif %>
                <% gob3 = gob3  %>
              <%end%>
            <%elsif%>
                <% gob3 = 0.0 %>
            <%end%>
          <%end%>
        <% elsif resultado.question.subcat.nombre == 'GOB4. Reportabilidad y Transparencia'%>
            <% valor = resultado.respuesta.to_f %>
            <%gob4 = gob4 + valor %>
        <% elsif resultado.question.subcat.nombre == 'GOB5. Huella de Carbono'%>
              <% valor = resultado.respuesta.to_f %>
              <%gob5 = gob5 + valor %>
       <% elsif resultado.question.subcat.nombre == 'GOB6. Coordinación Sustentabilidad'%>
                  <% valor = resultado.respuesta.to_f %>
                  <%gob6 = gob6 + valor %>
        <%end%>
      <% end %>
    <% end %>
  <% end %>
  <%end%>
